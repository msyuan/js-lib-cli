{
  "name": "@msyuan/js-lib-cli",
  "version": "1.0.5",
  "description": "一个快速创建JavaScript库开发项目的CLI工具",
  "author": "Lao Yuan",
  "license": "MIT",
  "private": false,
  "type": "module",
  "main": "bin/index.js",
  "keywords": [
    "library",
    "jscli",
    "cli",
    "js-cli",
    "js-lib",
    "js-lib-cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msyuan/js-lib-cli.git"
  },
  "bugs": {
    "url": "https://github.com/msyuan/js-lib-cli/issues"
  },
  "homepage": "https://github.com/msyuan/js-lib-cli#readme",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint-prettier": "prettier --write utils/",
    "test": "vitest",
    "cover": "vitest --coverage",
    "ui": "vitest --coverage --ui",
    "clone": "vitest clone --coverage",
    "copy": "vitest copy  --coverage",
    "merge": "vitest merge --coverage --ui",
    "test-init": "vitest init --coverage --ui"
  },
  "bin": {
    "js": "bin/index.js",
    "js-local": "bin/index-local.js"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "copy-dir": "^1.3.0",
    "figlet": "^1.6.0",
    "inquirer": "^9.2.11",
    "lodash": "^4.17.21",
    "ora": "^6.3.1",
    "template_js": "^2.8.1",
    "validate-npm-package-name": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.6",
    "@vitest/ui": "^0.34.6",
    "prettier": "^3.0.3",
    "vitest": "^0.34.6"
  },
  "engines": {
    "node": ">= 18.15.0"
  }
}
